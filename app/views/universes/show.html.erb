<h1 align="center"><%= @universe.name %></h1>
<br>
<br>
<br>
<br>
<br>
<%= link_to "Back to objects index", universes_path, :class=>"btn btn-sm color-white" %>
<% if logged_in? && @universe.users.include?(current_user) then %>
	<%= link_to "Edit object", edit_universe_path(@universe), :class=>"btn btn-sm color-white" %>
<% end %>
<hr>
<h4>Universe's users</h4>
<% @universe.users.each do |user| %>
	<%= user.id %>
	<%= user.username %>
	<br>
<% end %>
<h4>Universe's objs</h4>
<% @universe.objs.each do |obj| %>
	<%= obj.name %>
	<br>
<% end %>