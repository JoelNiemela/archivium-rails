<br>
<% @objs.each do |obj| %>
	<%= obj.id %>
	<%= obj.name %>
	<%= obj.obj_type %>
	<% if obj.users.include?(current_user) then %>
		<%= link_to "View", obj_path(obj), :class=>"btn btn-xs color-success" %>
		<%= link_to "Edit", edit_obj_path(obj), :class=>"btn btn-xs color-primary" %>
		<%= link_to "Delete", obj_path(obj), method: :delete, data: {confirm: "Are you sure you want to delete this object?"}, :class=>"btn btn-xs color-danger" %>
	<% end %>
	<br>
	<br>
<% end %>
<hr>
<% if logged_in? then %>
	<%= link_to "New object", new_obj_path, :class=>"btn btn-sm color-white" %>
<% end %>